{
  "name": "pohlang",
  "displayName": "PohLang Language Support",
  "description": "PohLang v0.6.0 language support: syntax highlighting, IntelliSense, snippets, and code completion (requires PLHub extension for code execution)",
  "version": "0.6.6",
  "publisher": "pohlang",
  "license": "MIT",
  "icon": "images/POHLANG.png",
  "galleryBanner": {
    "color": "#1e1e1e",
    "theme": "dark"
  },
  "engines": {
    "vscode": "^1.70.0"
  },
  "categories": [
    "Programming Languages",
    "Snippets",
    "Other"
  ],
  "keywords": [
    "pohlang",
    "poh",
    "interpreter",
    "programming language",
    "rust",
    "phrasal expressions",
    "natural language",
    "beginner-friendly"
  ],
  "main": "./out/extension.js",
  "activationEvents": [
    "onLanguage:pohlang"
  ],
  "contributes": {
    "languages": [
      {
        "id": "pohlang",
        "aliases": [
          "PohLang",
          "poh"
        ],
        "extensions": [
          ".poh"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "pohlang",
        "scopeName": "source.pohlang",
        "path": "./syntaxes/pohlang.tmLanguage.json"
      }
    ],
    "snippets": [
      {
        "language": "pohlang",
        "path": "./snippets/pohlang.json"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.70.0",
    "@types/node": "16.x",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "eslint": "^8.20.0",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "extract-zip": "^2.0.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pohlang/pohlang.git"
  },
  "bugs": {
    "url": "https://github.com/pohlang/pohlang/issues"
  },
  "homepage": "https://github.com/pohlang/pohlang#readme"
}
